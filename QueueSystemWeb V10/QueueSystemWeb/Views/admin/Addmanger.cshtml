@model QueueSystemWeb.Models.user
@{
    ViewBag.Title = "Adduser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm("Addmanger", "admin", FormMethod.Post, new { @id = "msform", @class = "adminpro-form", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    
    <div class="header-top-area">
        <div class="fixed-header-top">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-1 col-md-6 col-sm-6 col-xs-12">
                        <button type="button" id="sidebarCollapse" class="btn bar-button-pro header-drl-controller-btn btn-info navbar-btn">
                            <i class="fa fa-bars"></i>
                        </button>
                        <div class="admin-logo logo-wrap-pro">
                            <a href="#">
                                <img src="@Url.Content("~/Content/project/")img/logo/log.png" alt="" />
                            </a>
                        </div>
                    </div>

                    <div class="col-lg-6 col-md-1 col-sm-1 col-xs-12">
                        <div class="header-top-menu tabl-d-n">
                            <ul class="nav navbar-nav mai-top-nav">
                                <li class="nav-item">
                                    <a href="~/admin/AddMangerForEditOrg" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                        <i class="fa big-icon fa-flask"></i>
                                        <span class="mini-dn">اضافه مدير</span>
                                        <span class="indicator-right-menu mini-dn"></span>
                                    </a>
                                </li>
                           
                                <li class="nav-item dropdown">
                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                        عرض <span class="angle-down-topmenu"><i class="fa fa-angle-down"></i></span>
                                    </a>

                                    <div role="menu" class="dropdown-menu animated flipInX">
                                        <a href="~/admin/ShowmanBlocked" class="dropdown-item">المديرين المحظورين</a>
                                        <a href="~/admin/ShowmanunBlocked" class="dropdown-item">المديرين الغير المحظورين</a>
                                        <a href="~/admin/showorg" class="dropdown-item">المنظمات</a>


                                    </div>
                                </li>
                                <li class="nav-item">
                                    <a href="~/admin/adminreport" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                        <i class="fa big-icon fa-flask"></i>
                                        <span class="mini-dn">التقرير</span>
                                        <span class="indicator-right-menu mini-dn"></span>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a href="~/admin/EditProfile" role="button" aria-expanded="false">
                                        <i class="fa big-icon fa-edit"></i>
                                        <span class="mini-dn">تعديل الصفحه</span>
                                        <span class="indicator-right-menu mini-dn"></span>
                                    </a>
                                </li>

                                <li class="nav-item">
                                    <a href="~/admin/Index" role="button" aria-expanded="false">
                                        <i class="fa big-icon fa-backward"></i>
                                        <span class="mini-dn">رجوع</span>
                                        <span class="indicator-right-menu mini-dn"></span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-5 col-md-5 col-sm-6 col-xs-12">
                        <div class="header-right-info">
                            <ul class="nav navbar-nav mai-top-nav header-right-menu">

                                <li class="nav-item">
                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                        <span class="adminpro-icon adminpro-user-rounded header-riht-inf"></span>


                                    </a>
                                    <ul role="menu" class="dropdown-header-top author-log dropdown-menu animated flipInX">
                                        <li>
                                            <a href="~/employee/Index"><span class="adminpro-icon adminpro-user-rounded author-log-ic"></span>الصفحه الشخصيه</a>
                                        </li>
                                        <li>
                                            <a href="/Home/Logout" style="margin-left:20px"><span class="adminpro-icon adminpro-locked author-log-ic"></span>تسجيل الخروج</a>
                                        </li>
                                    </ul>
                                </li>

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="login-form-area mg-t-30 mg-b-15">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-3"></div>
                <form id="adminpro-register-form" class="adminpro-form">
                    <div class="col-lg-6">
                        <div class="login-bg">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="logo">
                                        <a href="#">
                                            <img src="@Url.Content("~/Content/project/")img/logo/images2.jpg" alt="" />
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="login-title">
                                        <h1>اضافه مدير</h1>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="login-input-head">
                                        <p>الاسم بالكامل</p>
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class="login-input-area">
                                        <input type="text" id="name" name="name" required maxlength="50"/>
                                        @*@Html.EditorFor(model => model.name, new { htmlAttributes = new { @name = "name", @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.name, "", new { @class = "text-danger" })*@
                                        <i class="fa fa-user login-user"style="left:90px;top: 10px;"></i>
                                    </div>
                                </div>

                            </div>

                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="login-input-head">
                                        <p>اسم المستخدم</p>
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class="login-input-area">
                                        <input type="text" id="username" name="username" required maxlength="50" />
                                        @*@Html.EditorFor(model => model.username, new { htmlAttributes = new { @name = "username", @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.username, "", new { @class = "text-danger" })*@
                                        <i class="fa fa-user login-user"style="left:90px;top: 10px;"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="login-input-head">
                                        <p>عنوان البريد</p>
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class="login-input-area">
                                        <input type="email" id="mail" name="mail" required />
                                        @*@Html.EditorFor(model => model.mail, new { htmlAttributes = new { @name = "mail", @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.mail, "", new { @class = "text-danger" })*@
                                        <i class="fa fa-envelope login-user" aria-hidden="true"style="left:90px;top: 10px;"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="login-input-head">
                                        <p>كلمه السر </p>
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class="login-input-area">
                                        <input type="password" id="password" name="password" required="" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="يجب أن يحتوي على رقم واحد على الأقل وحرف كبير وصغير ، وعلى الأقل 8 أحرف أو أكثر   "/>
                                        @*@Html.EditorFor(model => model.password, new { htmlAttributes = new { @name = "password", @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.password, "", new { @class = "text-danger" })*@
                                         <i class="fa fa-lock login-user"style="left:90px;top: 10px;"></i>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="login-input-head">
                                        <p>رقم التلفون</p>
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class="login-input-area">
                                        <input type="text" id="phone" name="phone" required="" maxlength="11"/>
                                        @*@Html.EditorFor(model => model.phone, new { htmlAttributes = new { @name = "phone", @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.phone, "", new { @class = "text-danger" })*@
                                        <i class="fa fa-phone login-user" aria-hidden="true"style="left:90px;top: 10px;"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="login-input-head">
                                        <p>النوع</p>
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class="cart-group">
                                        <label class="radio" >
                                            @Html.RadioButtonFor(model => model.gender, "ذكر")
                                            @Html.ValidationMessageFor(model => model.gender, "", new { @class = "text-danger" })
                                           <i></i>ذكر 
                                        </label>
                                        <label class="radio">
                                            @Html.RadioButtonFor(model => model.gender, "انثى")<i ></i>انثي
                                            @Html.ValidationMessageFor(model => model.gender, "", new { @class = "text-danger" })
                                        </label>

                                    </div>
                                </div>
                              

                            </div>
                            <div class="row">
                                <div class="col-lg-4"></div>
                                <div class="col-lg-8">
                                    <div class="login-button-pro">

                                        <button type="submit" class="login-button login-button-rg" style="float:right" id="save">اضافه </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div class="col-lg-3"></div>
            </div>
        </div>
    </div>
