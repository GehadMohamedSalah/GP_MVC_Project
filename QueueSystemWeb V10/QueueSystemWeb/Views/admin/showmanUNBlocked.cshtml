@*<div class="pull-right search"><input class="form-control" type="text" placeholder="Search" style="margin-top:30px"></div>*@
<div class="row" style="margin-top:80px;margin-right:930px">
<div class="col-lg-6">
    <div class="breadcome-heading">
        <form role="search" class="">
            <input type="text" placeholder="بحث..." class="form-control">
            <a href=""><i style="margin-right:125px" class="fa fa-search"></i></a>
        </form>
    </div>
</div>

</div>
<div style="margin-right:50px">


    <div class="sparkline8-hd" style="margin-top:-60px;">
        <div class="main-sparkline8-hd">
            <h1>المديرين الغير محظورين</h1>

        </div>
    </div>
    <div class="sparkline8-graph">
        <div class="static-table-list">
            <table id="table" data-toggle="table" data-pagination="false" data-show-toggle="false" data-click-to-select="true" data-toolbar="#toolbar">

                <thead>
                    <tr>

                        <th>الاسم</th>
                        <th>البريد</th>
                        <th>حظر</th>

                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < ViewBag.numberRow; i++)
            {
                        <tr>
                            <td>@ViewBag.users[i].name</td>
                            <td>@ViewBag.users[i].mail</td>
                            <td>
                                <button type="submit" class="login-button login-button-lg" style="background-color:#b3b3b3">
                                    <i class="fa big-icon fa-unlock">
                                        @Html.ActionLink("حظر ", "blockmang", new { @id = @ViewBag.users[i].id },
                                        new { onclick = "return confirm('هل انت متأكد من انك تريد حظر المدير   " + @ViewBag.users[i].name + "?');" })
                                    </i>

                                </button>
                            </td>


                        </tr>
                    }
                </tbody>

            </table>
        </div>


    </div>

</div>