                                    <span class="indicator-right-menu mini-dn"></span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-5 col-md-5 col-sm-6 col-xs-12">
                    <div class="header-right-info">
                        <ul class="nav navbar-nav mai-top-nav header-right-menu">

                            <li class="nav-item">
                                <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                    <span class="adminpro-icon adminpro-user-rounded header-riht-inf"></span>


                                </a>
                                <ul role="menu" class="dropdown-header-top author-log dropdown-menu animated flipInX">
                                    <li>
                                        <a href="~/employee/Index"><span class="adminpro-icon adminpro-user-rounded author-log-ic"></span>الصفحه الشخصيه</a>
                                    </li>
                                    <li>
                                        <a href="/Home/Logout" style="margin-left:20px"><span class="adminpro-icon adminpro-locked author-log-ic"></span>تسجيل الخروج</a>
                                    </li>
                                </ul>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@using (Html.BeginForm("Addemp", "manager", FormMethod.Post, new { @id = "msform", @class = "adminpro-form", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
<div class="login-form-area mg-t-30 mg-b-15">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-3"></div>
            <form id="adminpro-register-form" class="adminpro-form">
                <div class="col-lg-6">
                    <div class="login-bg">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="logo">
                                    <a href="#">
                                        <img src="@Url.Content("~/Content/project/")img/logo/images2.jpg" alt="" />
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="login-title">
                                    <h1>اضافه موظف</h1>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="login-input-head">
                                    <p>الاسم بالكامل</p>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="login-input-area">
                                    @Html.EditorFor(model => model.name, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.name, "", new { @class = "text-danger" })
                                     <i class="fa fa-user login-user"style="left:90px"></i>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="login-input-head">
                                    <p>اسم المستخدم</p>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="login-input-area">
                                 @Html.EditorFor(model => model.username, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.username, "", new { @class = "text-danger" })
                                     <i class="fa fa-user login-user"style="left:90px"></i>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="login-input-head">
                                    <p>عنوان البريد</p>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="login-input-area">
                                    @Html.EditorFor(model => model.mail, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.mail, "", new { @class = "text-danger" })
                                     <i class="fa fa-envelope login-user" aria-hidden="true"style="left:90px"></i>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="login-input-head">
                                    <p>كلمه السر</p>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="login-input-area">
                                    @Html.EditorFor(model => model.password, new { htmlAttributes = new { @class = "form-control" , @type="password"} })
                                    @Html.ValidationMessageFor(model => model.password, "", new { @class = "text-danger" })
                                      <i class="fa fa-lock login-user"style="left:90px"></i>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="login-input-head">
                                    <p>رقم التلفون</p>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="login-input-area">
                                    @Html.EditorFor(model => model.phone, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.phone, "", new { @class = "text-danger" })
                                     <i class="fa fa-phone login-user" aria-hidden="true"style="left:90px"></i>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="login-input-head">
                                    <p>النوع</p>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="cart-group">
                                    <label class="radio">
                                        @Html.RadioButtonFor(model => model.gender, "ذكر")
                                        @Html.ValidationMessageFor(model => model.gender, "", new { @class = "text-danger" })
                                        <i></i>ذكر
                                    </label>
                                    <label class="radio">
                                        @Html.RadioButtonFor(model => model.gender, "انثى")<i ></i>انثي
                                        @Html.ValidationMessageFor(model => model.gender, "", new { @class = "text-danger" })
                                    </label>

                                </div>
                            </div>
                         
                        </div>
              

                        <div class="row">
                            <div class="col-lg-4"></div>
                            <div class="col-lg-8">
                                <div class="login-button-pro">

                                    <button type="button" id="save" class="login-button login-button-rg" style="float:right">اضافه </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="col-lg-3"></div>
        </div>
    </div>
</div>
}

<script>
    $(document).ready(function () {

        //reserve
        $("#save").click(function () {
            myData = $("#msform").serialize();
            $.ajax({
                type: "POST",
                url: "/manager/Addemp",
                data: myData,
                success: function (response) {
                    $("#msform").empty();
                    if (response.result == 0) {
                        alert("اسم المستخدم موجود بالفعل");
                        document.location.reload(true);
                        window.location.href = "Addemp";
                    }
                    else if (response.result == 1) {
                        alert("تم اضافة المستخدم");
                        document.location.reload(true);
                        window.location.href = "showempUNBlocked";
                    }
                    else {
                        alert("البيانات المدخلة خطأ");
                        document.location.reload(true);
                        window.location.href = "Addemp";
                    }

                }

            })

        })

    })
</script>