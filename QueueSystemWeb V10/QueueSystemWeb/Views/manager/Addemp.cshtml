                                    <a href="~/manager/showorg" class="dropdown-item">المنظمات</a>

                                </div>
                            <li class="nav-item">
                                <a href="~/manager/managerreport" role="button" aria-expanded="false">
                                    <i class="fa big-icon fa-edit"></i>
                                    <span class="mini-dn">التقرير</span>
                                    <span class="indicator-right-menu mini-dn"></span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="~/manger/EditProfile" role="button" aria-expanded="false">
                                    <i class="fa big-icon fa-edit"></i>
                                    <span class="mini-dn">تعديل الصفحه</span>
                                    <span class="indicator-right-menu mini-dn"></span>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a href="~/manger/Index" role="button" aria-expanded="false">
                                    <i class="fa big-icon fa-backward"></i>
