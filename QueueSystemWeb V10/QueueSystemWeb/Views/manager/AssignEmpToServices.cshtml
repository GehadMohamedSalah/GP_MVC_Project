                                        </div>
                                    </div>
                                    <div class="col-lg-8">
                                        <div class="interested-input-area"style="margin-right:70px">
                                           @Html.DropDownListFor(model => model.branch_id,
           new SelectList(Model.branches, "id", "branch_location"), "--اختار فرع--",
           new { @class = "form-control", @style = "color:black;background-color:white;width:200px;" })
                                        @Html.ValidationMessageFor(model => model.branch_id, "", new { @class = "text-danger" })
