}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>
    $(document).ready(function () {

        //save
        $("#save").click(function () {
            myData = $("#msform").serialize();
            $.ajax({
                type: "POST",
                url: "/manager/AssignEmpToServices",
                data: myData,
                success: function (result) {
                    alert(result);
                    location.reload();
                }

            })

        })

    });


</script>